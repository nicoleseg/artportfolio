<%- include('../partials/header', {title:"Artist Details- TAP", user:user}) -%>
<h2>Update photo confirmation</h2>
<table>
  <tr>
    <th>
      Artist ID:
    </th>
  </tr>
  <tr>
    <th>
      <%=artist.id%>
    </th>
  </tr>
  <tr>
    <th>
      Photo ID:
    </th>
  </tr>
  <tr>
    <th>
      <%=photo.displayName%>
    </th>
  </tr>
</table>
<tr>
  <th>
    Update description Form:
  </th>
</tr>
<form action="/artists/<%=artist.id%>/<%=photo.displayName%>/u" enctype="multipart/form-data" method="POST">
  <label for="AID">Confirm Artist ID: </label>
  <input type="text" id="artistID" name="artistID"/><br>
  <label for="PD">Confirm Photo ID: </label>
  <input type="text" id="photoDisplayName" name="photoDisplayName"/><br>
  <label for="PD">New Description: </label>
  <input type="text" id="description" name="description"/><br>
  <br>
  <input type="submit" value="Update" required/>
</form>

<%- include('../partials/footer') -%>
