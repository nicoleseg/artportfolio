<%- include('../partials/header', {title:"Artist Details- TAP", user:user}) -%>

<h2>Artist - <%=artist.artistDisplayName%></h2>
<table>
  <tr>
    <th>
      Name:
    </th>
    <td>
      <%=artist.displayName%>
    </td>
  </tr>
  <tr>
    <th>
      Email Info:
    </th>
    <td>
      <%=artist.emailInfo%>
    </td>
  </tr>
  <tr>
  <%for(let i =0; i<artist.photos.length; i++){%>
  <th>
    <a href="/photos/<%=artist.photos[i].photoImage%>"><%artist.photos[i].displayName%></a>
  </th>
  <th>
    <img src="<%=artist.photos[i].photoImage%>" width='200'><br>
  </th>
  </tr>
  <%}%>
</table>

<%- include('../partials/footer') -%>
