<%- include('../partials/header', {title:"TAP: All Photos", user:user}) -%>

<h2>All Photos</h2>
<table>
  <tr>
    <th>
      Title
    </th>
    <th>
      Photo
    </th>
    <th>
      Description
    </th>
  </tr>
  <tr>
  <%for(let i =0; i<photos.length; i++){%>
  <th>
    <a href="/photos/<%=photos[i].image%>"><%=photos[i].displayName%></a>
  </th>
  <th>
    <img src="<%=photos[i].image%>" width='200'><br>
  </th>
  <th>
    <%=photos[i].description%>
  </th>
  </tr>
  <%}%>
</table>

  <%- include('../partials/footer') -%>
